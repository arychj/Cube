        <link rel="stylesheet" type="text/css" href="{{ asset('css/floorplan.css') }}" />
        <link rel="stylesheet" type="text/css" href="{{ asset('css/tiles.css') }}" />
        <link rel="stylesheet" type="text/css" href="{{ asset('css/sensors.css') }}" />
        <link rel="stylesheet" type="text/css" href="{{ asset('css/notices.css') }}" />
        <link rel="stylesheet" type="text/css" href="{{ asset('css/index.css') }}" />

        {% block css %}{% endblock %}
